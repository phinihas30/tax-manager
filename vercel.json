{
  "version": 2,
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "regions": ["bom1"],
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "env": {
    "NEXT_PUBLIC_SITE_URL": {
      "type": "production"
    },
    "NEXT_PUBLIC_SUPABASE_URL": {
      "type": "string"
    },
    "NEXT_PUBLIC_SUPABASE_ANON_KEY": {
      "type": "string"
    }
  }
} 